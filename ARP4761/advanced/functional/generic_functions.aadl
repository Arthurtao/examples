package functional::GenericFunctions
public
with WBSPS,EMV2,ARP4761;



system Generic
features
	isFailing : feature;
properties
	WBSPS::IsFunction => true;
annex EMV2 {**
	use types WBSErrorLib;
	use behavior WBSErrorLib::Twostate;
	error propagations
		isFailing : out propagation {WBError};
	flows
		f1 : error source isFailing {WBError};
	end propagations;
	
	component error behavior
	events
		Failure : error event;
	transitions
		t1 : Operational -[ Failure ]-> Failed;
	propagations
		p1 : Failed -[]-> isFailing (WBError);
	end component;
**};
end Generic;

system implementation Generic.basic
end Generic.basic;

end functional::GenericFunctions;